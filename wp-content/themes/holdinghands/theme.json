{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"custom": true,
			"palette": [
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#195641",
					"name": "Dark Green",
					"slug": "dark-green"
				},
				{
					"color": "#4aa775",
					"name": "Light Green",
					"slug": "light-green"
				},
				{
					"color": "#363636",
					"name": "Charcoal",
					"slug": "charcoal"
				},
				{
					"color": "#2d2d2d",
					"name": "Dark Charcoal",
					"slug": "dark-charcoal"
				}
			]
		},
		"layout": {
			"contentSize": "1100px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Roboto",
					"name": "Roboto",
					"slug": "roboto"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.9rem",
					"slug": "small"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": { 
		"elements": {
			"h1": {
				"typography": {
					"fontSize": ""
			}	
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}
